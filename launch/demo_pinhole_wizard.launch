<launch>

  <node name="display_server" pkg="freemoovr_engine" type="display_server" args="--mode overview --throttle">
    <rosparam command="load" file="$(find freemoovr_engine)/config/rosparamconfig.yaml" />
    <param name="p2g" type="str" value="$(find freemoovr_engine)/config/p2g.exr" />
    <param name="display/windowDecoration" type="bool" value="true" />
  </node>

  <node name="webserver" pkg="browser_joystick" type="web_control.py"/>
  <node name="joystick_cursor" pkg="freemoovr_engine" type="joystick_cursor"/>

</launch>
